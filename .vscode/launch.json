    "version": "0.2.0",
    "configurations": [
        {
            "type": "firefox",
            "request": "attach",
            "name": "Firefox launch",
            "webRoot": "https://localhost:9003",
            "url": "https://{instance}/{team-project}/_apps/hub/{plugin}",
            "pathMappings": [
                {
                    "url": "https://localhost:9003/",
                    "path": "${workspaceFolder}\\dist\\dev\\deploy/"
                },
                {
                    "url": "webpack:///src/",
                    "path": "${workspaceFolder}\\src/"
                }
            ]
        }
    ]
}